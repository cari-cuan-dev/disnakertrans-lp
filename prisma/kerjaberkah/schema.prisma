generator client {
  provider = "prisma-client"
  output   = "../../lib/generated/kerjaberkah-prisma"
}

datasource db {
  provider = "postgresql"
}

model cache {
  key        String @id @db.VarChar(255)
  value      String
  expiration Int
}

model cache_locks {
  key        String @id @db.VarChar(255)
  owner      String @db.VarChar(255)
  expiration Int
}

model failed_jobs {
  id         BigInt   @id @default(autoincrement())
  uuid       String   @unique(map: "failed_jobs_uuid_unique") @db.VarChar(255)
  connection String
  queue      String
  payload    String
  exception  String
  failed_at  DateTime @default(now()) @db.Timestamp(0)
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model hexa_role_user {
  role_id    BigInt
  user_id    BigInt
  hexa_roles hexa_roles @relation(fields: [role_id], references: [id], onDelete: NoAction, onUpdate: NoAction, map: "hexa_role_user_role_id_foreign")

  @@ignore
}

model hexa_roles {
  id              BigInt           @id @default(autoincrement())
  uuid            String           @unique(map: "hexa_roles_uuid_unique") @db.Uuid
  name            String           @db.VarChar(255)
  team_id         BigInt?
  created_by_name String?          @db.VarChar(255)
  access          Json?
  gates           Json?
  checkall        Json?
  guard           String           @default("web") @db.VarChar(255)
  created_at      DateTime?        @db.Timestamp(0)
  updated_at      DateTime?        @db.Timestamp(0)
  hexa_role_user  hexa_role_user[] @ignore
}

model job_batches {
  id             String  @id @db.VarChar(255)
  name           String  @db.VarChar(255)
  total_jobs     Int
  pending_jobs   Int
  failed_jobs    Int
  failed_job_ids String
  options        String?
  cancelled_at   Int?
  created_at     Int
  finished_at    Int?
}

model jobs {
  id           BigInt @id @default(autoincrement())
  queue        String @db.VarChar(255)
  payload      String
  attempts     Int    @db.SmallInt
  reserved_at  Int?
  available_at Int
  created_at   Int

  @@index([queue], map: "jobs_queue_index")
}

model migrations {
  id        Int    @id @default(autoincrement())
  migration String @db.VarChar(255)
  batch     Int
}

model password_reset_tokens {
  email      String    @id @db.VarChar(255)
  token      String    @db.VarChar(255)
  created_at DateTime? @db.Timestamp(0)
}

model sessions {
  id            String  @id @db.VarChar(255)
  user_id       BigInt?
  ip_address    String? @db.VarChar(45)
  user_agent    String?
  payload       String
  last_activity Int

  @@index([last_activity], map: "sessions_last_activity_index")
  @@index([user_id], map: "sessions_user_id_index")
}

model users {
  id                                    BigInt      @id @default(autoincrement())
  type                                  String      @default("Employee") @db.VarChar(255)
  name                                  String      @db.VarChar(255)
  email                                 String      @unique(map: "users_email_unique") @db.VarChar(255)
  email_verified_at                     DateTime?   @db.Timestamp(0)
  password                              String      @db.VarChar(255)
  remember_token                        String?     @db.VarChar(100)
  created_at                            DateTime?   @db.Timestamp(0)
  updated_at                            DateTime?   @db.Timestamp(0)
  companies_companies_created_byTousers companies[] @relation("companies_created_byTousers")
  companies_companies_deleted_byTousers companies[] @relation("companies_deleted_byTousers")
  companies_companies_updated_byTousers companies[] @relation("companies_updated_byTousers")
  companies_companies_user_idTousers    companies[] @relation("companies_user_idTousers")
  vacancies_vacancies_created_byTousers vacancies[] @relation("vacancies_created_byTousers")
  vacancies_vacancies_deleted_byTousers vacancies[] @relation("vacancies_deleted_byTousers")
  vacancies_vacancies_updated_byTousers vacancies[] @relation("vacancies_updated_byTousers")
  workers_workers_created_byTousers     workers[]   @relation("workers_created_byTousers")
  workers_workers_deleted_byTousers     workers[]   @relation("workers_deleted_byTousers")
  workers_workers_updated_byTousers     workers[]   @relation("workers_updated_byTousers")
  workers_workers_user_idTousers        workers[]   @relation("workers_user_idTousers")
}

model companies {
  id                                BigInt      @id @default(autoincrement())
  user_id                           BigInt?
  name                              String      @db.VarChar(255)
  business_registration_number      String      @db.VarChar(255)
  email                             String      @db.VarChar(255)
  phone                             String      @db.VarChar(255)
  website                           String?     @db.VarChar(255)
  address                           String
  city                              String      @db.VarChar(255)
  industrial_sector                 String      @db.VarChar(255)
  number_of_employees               String      @db.VarChar(255)
  description                       String?
  hr_name                           String      @db.VarChar(255)
  hr_position                       String      @db.VarChar(255)
  hr_phone                          String      @db.VarChar(255)
  hr_email                          String?     @db.VarChar(255)
  created_at                        DateTime?   @db.Timestamp(0)
  updated_at                        DateTime?   @db.Timestamp(0)
  deleted_at                        DateTime?   @db.Timestamp(0)
  created_by                        BigInt?
  updated_by                        BigInt?
  deleted_by                        BigInt?
  users_companies_created_byTousers users?      @relation("companies_created_byTousers", fields: [created_by], references: [id], onUpdate: NoAction, map: "companies_created_by_foreign")
  users_companies_deleted_byTousers users?      @relation("companies_deleted_byTousers", fields: [deleted_by], references: [id], onUpdate: NoAction, map: "companies_deleted_by_foreign")
  users_companies_updated_byTousers users?      @relation("companies_updated_byTousers", fields: [updated_by], references: [id], onUpdate: NoAction, map: "companies_updated_by_foreign")
  users_companies_user_idTousers    users?      @relation("companies_user_idTousers", fields: [user_id], references: [id], onUpdate: NoAction, map: "companies_user_id_foreign")
  vacancies                         vacancies[]
}

model personal_access_tokens {
  id             BigInt    @id @default(autoincrement())
  tokenable_type String    @db.VarChar(255)
  tokenable_id   BigInt
  name           String
  token          String    @unique(map: "personal_access_tokens_token_unique") @db.VarChar(64)
  abilities      String?
  last_used_at   DateTime? @db.Timestamp(0)
  expires_at     DateTime? @db.Timestamp(0)
  created_at     DateTime? @db.Timestamp(0)
  updated_at     DateTime? @db.Timestamp(0)

  @@index([expires_at], map: "personal_access_tokens_expires_at_index")
  @@index([tokenable_type, tokenable_id], map: "personal_access_tokens_tokenable_type_tokenable_id_index")
}

model vacancies {
  id                                BigInt     @id @default(autoincrement())
  company_id                        BigInt?
  title                             String     @db.VarChar(255)
  salary_start                      BigInt
  salary_end                        BigInt?
  type                              Json       @db.Json
  description                       String?
  benefits                          Json?      @db.Json
  created_at                        DateTime?  @db.Timestamp(0)
  updated_at                        DateTime?  @db.Timestamp(0)
  deleted_at                        DateTime?  @db.Timestamp(0)
  created_by                        BigInt?
  updated_by                        BigInt?
  deleted_by                        BigInt?
  companies                         companies? @relation(fields: [company_id], references: [id], onUpdate: NoAction, map: "vacancies_company_id_foreign")
  users_vacancies_created_byTousers users?     @relation("vacancies_created_byTousers", fields: [created_by], references: [id], onUpdate: NoAction, map: "vacancies_created_by_foreign")
  users_vacancies_deleted_byTousers users?     @relation("vacancies_deleted_byTousers", fields: [deleted_by], references: [id], onUpdate: NoAction, map: "vacancies_deleted_by_foreign")
  users_vacancies_updated_byTousers users?     @relation("vacancies_updated_byTousers", fields: [updated_by], references: [id], onUpdate: NoAction, map: "vacancies_updated_by_foreign")
}

model workers {
  id                              BigInt    @id @default(autoincrement())
  user_id                         BigInt?
  name                            String    @db.VarChar(255)
  skills                          Json      @db.Json
  skill                           String    @db.VarChar(255)
  email                           String    @db.VarChar(255)
  phone                           String    @db.VarChar(255)
  birth_date                      String    @db.VarChar(255)
  experience                      String    @db.VarChar(255)
  city                            String    @db.VarChar(255)
  address                         String
  education                       String    @db.VarChar(255)
  languages                       Json      @db.Json
  description                     String?
  certifications                  Json?     @db.Json
  created_at                      DateTime? @db.Timestamp(0)
  updated_at                      DateTime? @db.Timestamp(0)
  deleted_at                      DateTime? @db.Timestamp(0)
  created_by                      BigInt?
  updated_by                      BigInt?
  deleted_by                      BigInt?
  users_workers_created_byTousers users?    @relation("workers_created_byTousers", fields: [created_by], references: [id], onUpdate: NoAction, map: "workers_created_by_foreign")
  users_workers_deleted_byTousers users?    @relation("workers_deleted_byTousers", fields: [deleted_by], references: [id], onUpdate: NoAction, map: "workers_deleted_by_foreign")
  users_workers_updated_byTousers users?    @relation("workers_updated_byTousers", fields: [updated_by], references: [id], onUpdate: NoAction, map: "workers_updated_by_foreign")
  users_workers_user_idTousers    users?    @relation("workers_user_idTousers", fields: [user_id], references: [id], onUpdate: NoAction, map: "workers_user_id_foreign")
}
model headlines {
  id          BigInt    @id @default(autoincrement())
  title       String    @db.VarChar(255)
  description String?
  image       String?   @db.VarChar(255)
  status      Boolean?  @default(true)
  created_at  DateTime? @db.Timestamp(0)
  updated_at  DateTime? @db.Timestamp(0)
}

model introductions {
  id          BigInt    @id @default(autoincrement())
  title       String    @db.VarChar(255)
  description String?
  image       String?   @db.VarChar(255)
  details     Json?
  status      Boolean?  @default(true)
  created_at  DateTime? @db.Timestamp(0)
  updated_at  DateTime? @db.Timestamp(0)
}

model slides {
  id          BigInt    @id @default(autoincrement())
  title       String    @db.VarChar(255)
  description String?
  image       String    @db.VarChar(255)
  sort        Int       @default(0)
  status      Boolean   @default(true)
  created_at  DateTime? @db.Timestamp(0)
  updated_at  DateTime? @db.Timestamp(0)
}

model benefits {
  id           BigInt    @id @default(autoincrement())
  title        String    @db.VarChar(255)
  description  String?
  icon         String    @db.VarChar(255)
  border_color String?   @db.VarChar(255)
  sort         Int       @default(0)
  status       Boolean   @default(true)
  created_at   DateTime? @db.Timestamp(0)
  updated_at   DateTime? @db.Timestamp(0)
}

model join_steps {
  id          BigInt    @id @default(autoincrement())
  step_number String
  title       String    @db.VarChar(255)
  description String?
  bg_color    String?   @db.VarChar(255)
  sort        Int       @default(0)
  status      Boolean   @default(true)
  created_at  DateTime? @db.Timestamp(0)
  updated_at  DateTime? @db.Timestamp(0)
}
